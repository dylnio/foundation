{% for row in section %}
    <div class="apiresponse">
        <div class="responseheader">Response {{ loop.index }}</div>
        <div class="apiresponserow" style="display: none">
            <pre><code class="json">{{ json_decode(row.body)|json_encode(constant('JSON_PRETTY_PRINT')) }}</code></pre>
        </div>
    </div>
{% endfor %}
